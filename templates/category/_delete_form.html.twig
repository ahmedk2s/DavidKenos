<form method="post" action="{{ path('app_category_delete', {'id': category.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category.id) }}">
<button class="include">
<i class="fa-solid fa-trash"></i>
<span>Supprimer</span></button>

</form>
