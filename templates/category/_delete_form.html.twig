<form method="post" action="{{ path('app_category_delete', {'slug': category.slug}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet élément ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ category.slug) }}">
    <button class="btn">Supprimer</button>
</form>
