<form method="post" action="{{ path('app_news_delete', {'id': news.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer cet actualité ?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ news.id) }}">
<button class="include">
	<i class="fa-solid fa-trash"></i>
	<span>Supprimer</span>
</button>
</form>
